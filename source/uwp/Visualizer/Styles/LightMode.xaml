    <Color x:Key="ChromeLow">#F2F2F2</Color>
    <Color x:Key="ChromeAltLow">#2F2F2F</Color>

    <Color x:Key="CortanaAccent">#00AEEF</Color>
    <Color x:Key="CortanaAccentLow">#0078D7</Color>

    <Color x:Key="ErrorHigh">#D83B01</Color>
